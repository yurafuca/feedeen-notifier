{
    "name": "feedeenNotifier",
	"manifest_version": 2,
    "version": "1.0",
     "browser_action": {
	      "default_icon": "48.png",
	      "default_title": "open feedeen.com"
	 },
    "description": "Show unreed count of feedeen.com", 
    "icons": {
        "24": "24.png", 
        "48": "48.png", 
        "96": "96.png"
    }, 
    "permissions": [
        "unlimitedStorage", 
        "notifications", 
        "tabs",
        "background",
        "alarms"
    ],
    "background": {
        "persistent": true,
	    "page": "background.html"
	},
    "content_scripts": [
	{
	    "matches": ["*://www.feedeen.com/*"],
	    "js": ["jquery-2.1.4.js", "content.js"],
        "run_at": "document_end"
	}
    ],
    "web_accessible_resources": [
        "feedeen.html",
        "feedeen.js"
  ]
}